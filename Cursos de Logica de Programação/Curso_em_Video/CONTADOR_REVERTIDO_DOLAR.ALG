algoritmo "contador_revertido_dolar"
var
d1,c,d:real
q:inteiro
inicio
escreva("Quantas conversões serão realizadas: ")
leia(q)
c<-1
    enquanto (c<=q) faca
    escreva("Qual valor em R$ ")
    leia(d1)
    d<- d1/5,10
    escreval("O valor convertido em US$ ", d:3:2)
    c <- c+1
fimenquanto

fimalgoritmo